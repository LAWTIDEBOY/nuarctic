&simulationname
    runid='test'
/

&timestep
    step_per_day=48
    run_length=422
    run_length_unit='d'
/

&clockinit
    timenew=0.0
    daynew=105
    yearnew=2019
/

&meshproperties
    meshname='REcoM1D_mesh.nc'
/

&forcingproperties
    forcingname='REcoM1D_forcing.nc'
    atmdepositionname='atm_deposition.nc'
    tracerinitname='tracer_initialization.nc'
/

&diagnostics
    diag_freq       = 1          !only required for d,h,s cases,  y, m take 1
    diag_freq_unit  = 'd'        !output period: y,  d, h, s 
/

&calendar
    include_fleapyear=.true.
/


